  "version": "0.2.0",
  "configurations": [

    
    {
      "type": "node",
      "request": "launch",
      "name": "Start MCP server",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/upsun-mcp/src/index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/upsun-mcp/build/**/*.js"
      ]
    },
    { 
      "type": "debugpy",
      "name": "Chainlit",
      "request": "launch",
      "program": "${workspaceFolder}/chainlit/app.py",
      "env": {
      }
    }
  ]
}
